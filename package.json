    "axios": "0.26.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.5",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-datepicker": "^4.7.0",
    "react-dom": "^18.0.0",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.3.1",
    "react-paginate": "^8.1.3",
    "react-query": "^3.39.3",
    "react-redux": "^7.2.8",
    "react-resize-detector": "^7.1.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.7",
